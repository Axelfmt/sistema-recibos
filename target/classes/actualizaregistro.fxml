<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ComboBox?>
<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="amt.software.vistacontrolador.ActualizaRegistro">
    <headerText>Llena los campos</headerText>
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Cláusula:" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <ComboBox fx:id="comboBox" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <Label text="Día:" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="campoDia" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <Label text="Name:" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
            <TextField fx:id="campoCliente" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
            <Label text="Vehiculo:" GridPane.rowIndex="4" GridPane.columnIndex="0"/>
            <TextField fx:id="vehiculo" GridPane.rowIndex="4" GridPane.columnIndex="1"/>
            <Label text="Placas:" GridPane.rowIndex="5" GridPane.columnIndex="0"/>
            <TextField fx:id="campoPlacas" GridPane.rowIndex="5" GridPane.columnIndex="1"/>
            <Label text="Tarifa:" GridPane.rowIndex="6" GridPane.columnIndex="0"/>
            <TextField fx:id="campoTarifa" GridPane.rowIndex="6" GridPane.columnIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
